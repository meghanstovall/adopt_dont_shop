<a href="/shelters">Shelter Index</a>

<h1>Shelter ID: <%= @shelter.id %></h1>

<p>Shelter: <%= @shelter.name %></p>
<p>Address: <%= @shelter.address %></p>
<p>City: <%= @shelter.city %></p>
<p>State: <%= @shelter.state %></p>
<p>Zipcode: <%= @shelter.zip %></p>

<a href="/shelters/<%= @shelter.id %>/edit">Update Shelter</a>
<a href="/shelters/<%= @shelter.id %>/pets">Pets</a>



<%= form_tag "/shelters/#{@shelter.id}", method: :delete do %>
  <%= submit_tag(value="Delete") %>
<% end %>
